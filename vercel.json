{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/index.js": {
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}